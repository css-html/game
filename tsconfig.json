{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "outDir": "build",
    "module": "esnext",
    "target": "es5",
    "lib": [
      "es6",
      "dom",
      "es2016",
      "es2017"
    ],
    "sourceMap": true,
    "allowJs": false,
    "jsx": "react-jsx",
    "declaration": true,
    "declarationDir": "build/dts",
    "declarationMap": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "baseUrl": "./src"
  },
  "include": [
    "src/"
, "src/components/bits/Range/.stories.tsx", "src/components/bits/Range/.stories.tsx", "src/components/bits/Range/.stories.tsx"  ],
  "exclude": [
    "node_modules",
    "build",
    "**/*.stories.tsx"
    // "src/pages"
  ]
}
